	<xs:element name="TransactionPostingDate" type="SAFdateType">
		<xs:annotation>
			<xs:documentation>Buchungsdatum - Datum der Verbuchung, wenn es nicht dem Belegdatum entspricht</xs:documentation>
			<xs:documentation>Date of posting of the transaction if different to Transaction Date</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TransactionPostingTime" type="xs:time">
		<xs:annotation>
			<xs:documentation>Buchungszeit</xs:documentation>
			<xs:documentation>Time of posting of the transaction</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TransactionReference" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>BelegBezug - Eindeutige Refernz zum Geschäftsfall</xs:documentation>
			<xs:documentation>Unique reference to the transaction.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TransactionSubType" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Untergliederung des Geschäftsfalles</xs:documentation>
			<xs:documentation>Indentify type of transaction on line level: efficiencyloss, use of components in production, etc. Predescribed TABLE is possible.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TransactionType" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Art des Geschäftes (Vordefinition in Tabelle möglich) - Eingabe der Codes 1 - 9 der Art des Geschäftes für Intrastat-Meldung </xs:documentation>
			<xs:documentation>Identify sale, purchase, adjustment, movement etc. Predescribed TABLE is possible.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Transfers" type="SAFmonetaryType">
		<xs:annotation>
			<xs:documentation>Umbuchungen</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UCR" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>UCR - Unique consignment reference number (Einheitliche lieferungsbezogene Referenznummer - Zoll (WCO))</xs:documentation>
			<xs:documentation>Unique consignment reference number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UnitOfMeasure" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Verpackungseinheit</xs:documentation>
			<xs:documentation>Quantity unit of measure e.g. pack of 12</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UnitOfMeasureDescription" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Bezeichnung Verpackungseinheit</xs:documentation>
			<xs:documentation>Description of the UOM</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UnitOfMeasurePhysicalStock" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Maßeinheit Inventur</xs:documentation>
			<xs:documentation>Unit of Measurement for this Physical Stock position</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UnitPrice" type="SAFmonetaryType">
		<xs:annotation>
			<xs:documentation>Einzelpreis der Ware (je Masseinheit)</xs:documentation>
			<xs:documentation>Unit price for the unit/group of units per UOM</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UOMBase" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Mengeneinheit Basis - Mengeneinheit für Lagerverwaltung für dieses Produkt (vordefinierte Tabelle ist möglich)</xs:documentation>
			<xs:documentation>Unit of measure for Stock Administration for this product Predescribed TABLE is possible.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UOMPurchase" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Mengeneinheit Einkauf</xs:documentation>
			<xs:documentation>Unit of Measure - Purchase</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UOMSale" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Mengeneinheit Verkauf</xs:documentation>
			<xs:documentation>Unit of Measure - Sale</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UOMStandard" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Maßeinheit, z. B. Kilo, Meter, Liter, etc.</xs:documentation>
			<xs:documentation>A Standard Unit of Measure applicable for this product, f.i. Kilo, Metres, Litres (Predescribed TABLE is possible)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UOMToUOMBaseConversionFactor" type="SAFdecimalType">
		<xs:annotation>
			<xs:documentation>Umrechnungsfaktor der Maßeinheit</xs:documentation>
			<xs:documentation>Conversion factor of the UOM to UOM Base</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UOMToUOMPhysicalStockConversionFactor" type="SAFdecimalType">
		<xs:annotation>
			<xs:documentation>Umrechnungsfaktor Maßeinheit Inventur</xs:documentation>
			<xs:documentation>Conversion factor of the UOM to UOM Physical Stock</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ValuationMethod" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Bewertungsverfahren, z. B. FIFO, LIFO, Durchschnittspreisverfahren, etc.</xs:documentation>
			<xs:documentation>FIFO, LIFO, Average cost etc.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ValutaDate" type="SAFdateType">
		<xs:annotation>
			<xs:documentation>Eingabe Valutadatum, wenn Abweichung von Belegdatum</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="WarehouseID" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Lager(haus)Nr/-bezeichnung; Bezeichnung des Lagers, wo sich die Ware befindet (inkl. Waren in Bearbeitung oder unterwegs befindliche Waren) </xs:documentation>
			<xs:documentation>Warehouse where goods held - also to identify work-in-progress, or stock-in-transit</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="WarehouseLocation" type="AddressStructure">
		<xs:annotation>
			<xs:documentation>Warehouse location</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Website" type="xs:anyURI">
		<xs:annotation>
			<xs:documentation>Homepage des Unternehmens</xs:documentation>
			<xs:documentation>Company website URL</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="SAFdateType">
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<xs:simpleType name="SAFdecimalType">
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="SAFmonetaryType">
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="SAFtextType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
</xs:schema>
