<xs:schema xmlns="urn:OECD:StandardAuditFile-Taxation:AT_1.01" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:doc="urn:schemas-OECD:schema-extensions:documentation" targetNamespace="urn:OECD:StandardAuditFile-Taxation:AT_1.01" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.00" id="SAF-T_AT">
	<xs:annotation>
		<xs:documentation>
			<doc:Title>OECD Standard Audit File:AT_1.01</doc:Title>
			<doc:Copyright>Erstellt vom BMF</doc:Copyright>
			<doc:Subject>Standard Audit File - Taxation Austria</doc:Subject>
			<doc:Version>
				<doc:Number>1.01 Final Basis: OECD SAF-T 1.93</doc:Number>
				<doc:Status>Final AT</doc:Status>
			</doc:Version>
			<doc:Author>RIA: Silke Frisch, Heidelinde Eberhart-Muth, Karl Nachtnebel, Bernhard Pfeifer, Romana Reiterer, Wolfgang Reiterer</doc:Author>
			<doc:ModificationDate>31-01-2009</doc:ModificationDate>
		</xs:documentation>
	</xs:annotation>
	<xs:complexType name="AddressStructure">
		<xs:sequence>
			<xs:element ref="BuildingNumber"/>
			<xs:element ref="StreetName">
				<xs:annotation>
					<xs:documentation>Strasse, wenn nicht vorhanden Angabe des Ortsteiles bzw. Ortsbezeichnung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="AddressDetail" minOccurs="0"/>
			<xs:element ref="City"/>
			<xs:element ref="PostalCode"/>
			<xs:element ref="Region" minOccurs="0"/>
			<xs:element ref="Country"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContactInformationType">
		<xs:sequence>
			<xs:element ref="Contact"/>
			<xs:element ref="Telephone"/>
			<xs:element ref="Fax" minOccurs="0"/>
			<xs:element ref="Email" minOccurs="0"/>
			<xs:element ref="Website" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ShippingPointStructure">
		<xs:sequence>
			<xs:element ref="DeliveryID" minOccurs="0"/>
			<xs:element ref="DeliveryDate" minOccurs="0"/>
			<xs:element ref="WarehouseID" minOccurs="0"/>
			<xs:element ref="LocationID" minOccurs="0"/>
			<xs:element ref="UCR" minOccurs="0"/>
			<xs:element ref="Address" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaxIDstructure">
		<xs:sequence>
			<xs:element ref="TaxID" minOccurs="0"/>
			<xs:element ref="TaxType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Steuerart (OECD-Vorgabe)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="TaxCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Steuercode (OECD-Vorgabe)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="TaxVerificationDate" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaxTableType">
		<xs:sequence>
			<xs:element ref="TaxType"/>
			<xs:element ref="Description">
				<xs:annotation>
					<xs:documentation>Bezeichnung Steuerart</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxCodeDetails" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="TaxCode"/>
						<xs:element ref="Description">
							<xs:annotation>
								<xs:documentation>Bezeichnung Steuercode</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element ref="TaxPercentage"/>
						<xs:element ref="EffectiveDate" minOccurs="0">
							<xs:annotation>
								<xs:documentation>(OECD-Vorgabe)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="AccountDescription" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Kontobezeichnung</xs:documentation>
			<xs:documentation>Name of individual GL account</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AccountID" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Kontonummer</xs:documentation>
			<xs:documentation>General Ledger Account code</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AccountType" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Kontenart - Aktives bzw. passives Bestandskonto, Ertragskonto, Aufwandskonto</xs:documentation>
			<xs:documentation>Type of account  - Asset/Liability/Sale/Expense</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AccumulatedDepreciation" type="SAFmonetaryType">
		<xs:annotation>
			<xs:documentation>Kumulierte AfA</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AcquisitionAndProductionCosts" type="SAFmonetaryType">
		<xs:annotation>
			<xs:documentation>Anschaffungs- und Herstellungskosten</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Address" type="AddressStructure">
		<xs:annotation>
			<xs:documentation>Adresse</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AddressDetail" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Adresszusatz</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AppreciationInValue" type="SAFmonetaryType">
		<xs:annotation>
			<xs:documentation>Betrag der Zuschreibung</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Analysis">
		<xs:annotation>
			<xs:documentation>(OECD-Vorgabe)</xs:documentation>
			<xs:documentation/>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AnalysisID" minOccurs="0"/>
				<xs:element ref="Description" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AnalysisID" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>(OECD-Vorgabe)</xs:documentation>
			<xs:documentation>Analysis code identifier</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AssetAddition" type="SAFmonetaryType">
		<xs:annotation>
			<xs:documentation>Zugang Anlagegüter im Wirtschaftsjahr</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AssetDescription" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Anlagenbezeichnung</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AssetDisposal" type="SAFmonetaryType">
		<xs:annotation>
			<xs:documentation>Abgang Anlagegüter im Wirtschaftsjahr</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AssetID" type="SAFtextType">
		<xs:annotation>
			<xs:documentation>Anlagennummer</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AssetLifeYear" type="SAFdecimalType">
		<xs:annotation>
			<xs:documentation>Nutzungsdauer</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AssetStatement">
		<xs:annotation>
			<xs:documentation>Anlagenverzeichnis</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="NumberOfAssets" type="xs:integer">
					<xs:annotation>
						<xs:documentation>Anzahl der Anlagegüter</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TotalAcquisitionAndProductionCosts" type="SAFmonetaryType">
					<xs:annotation>
						<xs:documentation>Gesamtsumme der Anschaffungs- und Herstellungskosten</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TotalBookValueEnd" type="SAFmonetaryType">
					<xs:annotation>
						<xs:documentation>Gesamtsumme der Buchwerte am Ende des Jahres</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Asset" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="AccountID">
								<xs:annotation>
									<xs:documentation>Konto Nummer der Verbuchung des Anlagegutes</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="AssetID"/>
							<xs:element ref="AssetDescription"/>
							<xs:element ref="DateOfAcquisition"/>
							<xs:element name="SupplierAsset">
								<xs:annotation>
									<xs:documentation>Angaben zum Lieferanten des Anlagengutes</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="SupplierName"/>
										<xs:element ref="SupplierID" minOccurs="0"/>
										<xs:element ref="Address"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element ref="StartUpDate" minOccurs="0"/>
							<xs:element ref="AcquisitionAndProductionCosts"/>
							<xs:element ref="AssetAddition" minOccurs="0"/>
							<xs:element ref="Transfers" minOccurs="0"/>
							<xs:element ref="AssetDisposal" minOccurs="0"/>
							<xs:element name="AcquisitionAndProductionCostsEnd" type="SAFmonetaryType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Anschaffungs- und Herstellungskosten zum Ende des Wirtschaftsjahres</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="AssetLifeYear">
								<xs:annotation>
									<xs:documentation>Nutzungsdauer in Jahren</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="AssetLifeMonth" type="SAFdecimalType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Nutzungsdauer in Monaten</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="DepreciationMethod" type="SAFtextType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Art der Absetzung für Abnutzung im laufenden Wirtschaftsjahr</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="DepreciationPercentage" type="SAFdecimalType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>AfA-Prozentsatz</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="BookValueBegin" minOccurs="0"/>
							<xs:element ref="AppreciationInValue" minOccurs="0"/>
							<xs:element ref="DepreciationForYear"/>
							<xs:element name="ExtraordinaryDepreciationMethod" type="SAFtextType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Art der Sonderabschreibung</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="ExtraordinaryDepreciation" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Sonderabschreibungen im laufenden Geschäftsjahr</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="AdditionalExtraordinaryDepreciation" type="SAFmonetaryType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Weitere Sonderabschreibungen</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="BookValueEnd"/>
							<xs:element name="PurchaseOrderDate" type="SAFdateType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Datum der Bestellung eines Wirtschaftsgutes</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="InvestmentSupport" minOccurs="0"/>
							<xs:element ref="AccumulatedDepreciation" minOccurs="0"/>
							<xs:element name="AssetTransactions" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="AssetTransactionType" type="SAFtextType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Eine von der Software vorgegebene Nummer für eine Bewegungsart</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="AssetTransactionTypeDescription" type="SAFtextType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Bezeichnung der Bewegungsart (z. B. im Anlagevermögen: Zuschreibung für ein Wirtschaftsgut).</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="AssetTransactionDate" type="SAFdateType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Datum, unter dem eine Bewegungsart erfasst ist (zum Beispiel im Anlagevermögen: Datum, unter dem der Zugang eines Wirtschaftsgutes erfolgte).</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="AssetTransactionAmount" type="SAFmonetaryType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Wert der Buchung/Transaction</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="AssetSales" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="AcquisitionAndProductionCostsOnSale" type="SAFmonetaryType" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Wert der Anschaffungs- und Herstellungskosten eines Wirtschaftsgutes</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="BookValueOnSale" type="SAFmonetaryType">
														<xs:annotation>
															<xs:documentation>Restbuchwert des Wirtschaftsgutes zum Abgangszeitpunkt (Brutto AHK Wert abzüglich aufgelaufener AfA)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="NetSalesRevenue" type="SAFmonetaryType" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Wert des Erlöses eines verkauften Wirtschaftsgutes ohne USt (entspricht Umsatzerlös und nicht Buchgewinn o. -verlust)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="NetSalesEarnings" type="SAFmonetaryType" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Wert des Buchverlustes oder -gewinnes eines Wirtschaftsgutes bei Anlagenabgang</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="TransactionDateSale" type="SAFtextType" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Datum, das einen Datensatz bzw. eine Buchungstransaktion einer definierten Periode (Besteuerungszeitraum) zuordnet. In der Regel das Kalenderjahr, ein Quartal oder Monat. Muss nicht ausgefüllt werden, wenn es dem AssetTransactionDate entspricht.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AuditFile">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Header">
					<xs:annotation>
						<xs:documentation>Unternehmensstammdaten</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="MasterFiles">
					<xs:annotation>
						<xs:documentation>Stammdaten (Konten, Lieferanten, Kunden, etc.)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="GeneralLedgerEntries" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Buchungsjournal(e)</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="NumberOfEntries"/>
							<xs:element ref="TotalDebit"/>
							<xs:element ref="TotalCredit"/>
							<xs:element name="Journal" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="JournalID" type="SAFtextType">
											<xs:annotation>
												<xs:documentation>Eindeutige Kennung des Buchungsjournales, oder der Rechnungen und Zahlungen in einer Einnahmen-Ausgaben-Rechnung </xs:documentation>
												<xs:documentation>Source GL journal identifier, or invoices and payments in single ledger systems</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element ref="Description">
											<xs:annotation>
												<xs:documentation>Journalkennzeichnung</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Type" type="SAFtextType">
											<xs:annotation>
												<xs:documentation>Journaltyp/Journalart, z.B. Hauptbuchjournal, Fakturenjournal, ...</xs:documentation>
												<xs:documentation>Grouping mechanism for journals</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Transaction" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="GLPostingDate">
														<xs:annotation>
															<xs:documentation>Buchungsdatum</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element ref="TransactionDate"/>
													<xs:element ref="Period"/>
													<xs:element ref="TransactionCode" minOccurs="0"/>
													<xs:element ref="TransactionID">
														<xs:annotation>
															<xs:documentation>Belegnummer</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element ref="Description" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Buchungstext auf Transaktionsebene</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="CompanyCode" type="SAFtextType" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Buchungskreis, Abrechnungskreis</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element ref="CustomerID" minOccurs="0"/>
													<xs:element ref="SupplierID" minOccurs="0"/>
													<xs:element ref="BatchID" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Stapelverarbeitungsnummer</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="SourceID" type="SAFtextType" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Benutzerkennung; Angaben zur Person oder Anwendung, welche die den Geschäftsfall eingegeben hat (Namenskürzel)</xs:documentation>
															<xs:documentation>Details of person or application entered the transaction</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element ref="SystemID" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Systemerfassungsnummer</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Line" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element ref="RecordID">
																	<xs:annotation>
																		<xs:documentation>Eindeutige Datensatznummer bezogen aufs Journal/Laufende, eindeutige Nummerierung der Buchungszeilen bezogen auf das jeweilige Journal</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element ref="AccountID"/>
																<xs:element ref="ValutaDate" minOccurs="0"/>
																<xs:element ref="PostingText"/>
																<xs:element ref="Analysis" minOccurs="0" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation> (OECD-Vorgabe)</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="SourceDocumentID" type="SAFtextType" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Referenznummer Quelldokument</xs:documentation>
																		<xs:documentation>Source document number to which line relates</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="SystemEntryDate" type="SAFdateType" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Systemerfassungsdatum - Vom System generiertes Buchungsdatum (OECD-Vorgabe)</xs:documentation>
																		<xs:documentation>Date captured  by system</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:choice>
																	<xs:element ref="DebitAmount"/>
																	<xs:element ref="CreditAmount"/>
																</xs:choice>
																<xs:element ref="Tax" minOccurs="0" maxOccurs="unbounded"/>
																<xs:element ref="Currency" minOccurs="0"/>
																<xs:element ref="CostAccounting" minOccurs="0" maxOccurs="unbounded"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="InventoryStockLevels" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Inventurdaten</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SourceDocuments" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Quelldaten</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:choice maxOccurs="unbounded">
							<xs:element name="SalesInvoices" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="NumberOfEntries"/>
										<xs:element ref="TotalDebit"/>
										<xs:element ref="TotalCredit"/>
										<xs:element name="Invoice" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="InvoiceNo"/>
													<xs:element ref="Period" minOccurs="0"/>
													<xs:element ref="InvoiceDate"/>
													<xs:element ref="InvoiceType" minOccurs="0"/>
													<xs:element ref="AccountID" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Kontonummer Kundenkonto</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element ref="CustomerID" minOccurs="0"/>
													<xs:element ref="Source" minOccurs="0"/>
													<xs:element ref="GLPostingDate" minOccurs="0"/>
													<xs:element ref="BatchID" minOccurs="0"/>
													<xs:element ref="SystemID" minOccurs="0"/>
													<xs:element ref="TransactionCode" minOccurs="0"/>
													<xs:element ref="TransactionID" minOccurs="0"/>
													<xs:element name="Line" maxOccurs="unbounded">
														<xs:complexType>
